<?xml version="1.0" encoding="UTF-8"?>

<suite name="pt_halloween" thread-count="4" parallel="tests">
    <listeners>
        <listener class-name="com.milanoo.listener.MilanooIResultListener"/>
    </listeners>
    <parameter name="site" value="pt"/>
    <parameter name="project" value="halloween"/>
    <parameter name="pageUrl" value="http://www.milanoo.com/pt/search?type=search&amp;keyword="/>
    <test name="firefox_halloween">
        <parameter name="browser" value="firefox"/>
        <parameter name="version" value="47.0"/>
        <parameter name="platform" value="WINDOWS"/>
        <classes>
            <class name="com.milanoo.tests.TestHalloween"/>
        </classes>
    </test>
    <test name="chrome_halloween">
        <parameter name="browser" value="chrome"/>
        <parameter name="version" value="52.0"/>
        <parameter name="platform" value="WINDOWS"/>
        <classes>
            <class name="com.milanoo.tests.TestHalloween"/>
        </classes>
    </test>
</suite>
